
RewriteEngine On

#RewriteBase /openemr-importer2/contrib/dataImporter/public/
RewriteBase /openemr-importer2/
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
#was RewriteRule ^.*$ - [NC,L]
#was RewriteRule ^.*$ index.php [NC,L]
RewriteRule ^.*$ - [NC,L]
RewriteRule ^.*$ contrib/dataImporter/public/index.php [NC,L]
#RewriteRule ^(.*)/contrib/(.*)$ index.php [NC,L]

RewriteRule ^.*$ index.php [NC,L]
#RewriteEngine On
#RewriteRule ^(([^/]+/)*)link([^/]*)\.html$ http://www.example.com/$1$3.html [R=301,L]
#RewriteRule link([^/]*).html$ rewrite.php?link=$1 [R=301,L]
