
RewriteEngine On

#RewriteBase /openemr-412/modules/dataImporter/public/
RewriteBase /openemr-412/
RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
#was RewriteRule ^.*$ - [NC,L]
#was RewriteRule ^.*$ index.php [NC,L]
RewriteRule ^.*$ - [NC,L]
#RewriteRule ^.*$ - [NC,L]
RewriteRule ^.*$ modules/dataImporter/public/index.php [NC,L]
RewriteRule ^(.*)/modules/(.*)$ index.php [NC,L]

RewriteRule ^.*$ index.php [NC,L]
#RewriteEngine On
#RewriteRule ^(([^/]+/)*)link([^/]*)\.html$ http://www.example.com/$1$3.html [R=301,L]
#RewriteRule link([^/]*).html$ rewrite.php?link=$1 [R=301,L]
